<!DOCTYPE html>
<html>
  <head>

    <title>Mystore</title>
    <%= stylesheet_link_tag 'style','tn3' %>
    <%= javascript_include_tag 'application','controls','dragdrop','effects','jquery-1.6.4','jquery.tn3lite.min','prototype','rails','validation','magicthumb' %>
    <%= csrf_meta_tag %>
  </head>
  <body>
    <div class="inned_boby">
      <div class="main_wrap">
        <div class="top_nav">
          <ul>
            <li><a href="#"></a></li>
            <li><a href="#"></a></li>
            <li><a href="#"></a></li>
          </ul>
        </div>
        <div class="clear"></div>
        <div class="logo"><img src="../images/logo.png" width="309" height="95" /></div>
        <div class="nav_bg">
          <div class="nab">
            <ul>
              <li><%= link_to 'Home', root_path %></li>
              <% if !current_user.blank? %>
                <% if current_user.user_type=="STORE" && current_user.login!="admin" && current_user.status==true %>
                  <li><%= link_to "Add Item" , new_item_path   %></li>
                <% end %>
              <% end %>
              <% if !current_user.blank? %>
                <li> <%= link_to 'Logout', logout_path %></li>
              <% else %>
                <li><%= link_to "Login" , login_path   %></li>
              <% end %>
              <%#*<li><a href="#">Register</a></li>%>
              <li><%= link_to "Create Charity Or Club" , new_store_path   %></li>

            </ul>
          </div>
    
        </div>
              
</div>
        <%= yield %>
        <div class="footer ">
          <div class="main_wrap">
            <div class="FL" style="padding:20px 0 0 20px;"><img src="../images/logo.png" width="309" height="95" /></div>
            <div class=" FL top_nav" style="padding:40px 0 0 100px;">
              <ul>
                <li><a href="#">About Us</a>|</li>
                <li><a href="#">Contact</a>| </li>
                <li><a href="#">Help</a></li>
              </ul>
            </div>
            <div class="clear"></div>
            <p>&nbsp;</p>
          </div>
        </div>
      
    </div>
  </body>
</html>
